<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.0/css/font-awesome.min.css">
    <!-- Bootstrap core CSS -->
    <link href="../../public/css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link href="../../public/css/mdb.min.css" rel="stylesheet">
    <!-- Your custom styles (optional) -->
    <link href="../../public/css/style.css" rel="stylesheet">
    <link rel="icon" href="../../public/img/iitj.png">
    <title>Office of Students' Placement, IIT Jodhpur</title>
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <style type="text/css">
        .row {
            margin-left: 0;
            margin-right: 0;
        }
        
        .container {
            .row {
                margin-left: -15px;
                margin-right: -15px;
            }
        }
        
        #border1: {
            margin-top: 5px;
            margin-bottom: 5px;
            ;
        }
        
        li {
            margin: 10px;
        }
        
        li a {
            margin: 10px;
        }
    </style>
</head>

<body style="margin:0px auto ;">
    <h2 align="center">Office of Students' Placement, IIT Jodhpur</h2>
    <hr>
    <h1 align="center">Welcome
        <%= adminName %>
    </h1>
    <p align="center">
        <a class="btn btn-default" href="/admin/verifyResume">Verify Resume</a>
        <a class="btn btn-default" href="/admin/viewCredentials">Students Credentials</a>
        <a class="btn btn-default" href="/admin/viewCompany">View Companies</a>
        <a class="btn btn-default" href="/admin/logout">Logout</a>
    </p>
        <% for(var i = 0;i < data.length;i++) { %>
            <p style="margin-left:10%;">Company Id:<b> <%= unqid%> </b></p>
            <div class="row">
                <div class="col-lg-1"></div>
                <div class="col-lg-5">
                    <table cellspacing="7" cellpadding="7" align="center">
                        <th>Basic Details</th>
                        <tr>
                            <td>Company Name</td>
                            <td>
                                <%=data[i].name%>
                            </td>
                        </tr>
                        <tr>
                            <td>Email</td>
                            <td>
                                <%=data[i].email%>
                            </td>
                        </tr>
                        <tr>
                            <td>Type</td>
                            <td>
                                <%=data[i].type%>
                            </td>
                        </tr>
                        <tr>
                            <td>Website</td>
                            <td>
                                <%=data[i].url%>
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="col-lg-5">
                    <p><b>Complete Address:</b></p>
                    <p>City:
                        <%=data[i].city%>, State:
                            <%=data[i].state%>, Country:
                                <%=data[i].country%>, ZIP:
                                    <%=data[i].zip%>
                    </p>
                    <p><b>Offers Count :</b>&nbsp&nbsp
                        <%=data[i].offerscount%>
                    </p>
                </div>
                <div class="col-lg-1"></div>
            </div>
            <hr style="width:70%;">
            <div class="row">
                <div class="col-lg-1"></div>
                <div class="col-lg-3">
                    <table cellspacing="7" cellpadding="7" align="center" style="padding-left:10px;border-right:1px solid black;">
                        <p><b>Contact Person 1</b></p>
                        <tr>
                            <td>Name</td>
                            <td>
                                <%=data[i].person1_name%>
                            </td>
                        </tr>
                        <tr>
                            <td>Designation</td>
                            <td>
                                <%=data[i].person1_desg%>
                            </td>
                        </tr>
                        <tr>
                            <td>Mobile</td>
                            <td>
                                <%=data[i].person1_mobile%>
                            </td>
                        </tr>
                        <tr>
                            <td>Phone</td>
                            <td>
                                <%=data[i].person1_phone%>
                            </td>
                        </tr>
                        <tr>
                            <td>Email</td>
                            <td>
                                <%=data[i].person1_email%>
                            </td>
                        </tr>
                        <tr>
                            <td>Fax</td>
                            <td>
                                <%=data[i].person1_fax%>
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="col-lg-3">
                    <table cellspacing="7" cellpadding="7" align="center" style="padding-left:10px;border-right:1px solid black;">
                        <p><b>Contact Person 2</b></p>
                        <% if(data[i].person2_name){ %>
                            <tr>
                                <td>Name</td>
                                <td>
                                    <%=data[i].person2_name%>
                                </td>
                            </tr>
                            <tr>
                                <td>Designation</td>
                                <td>
                                    <%=data[i].person2_desg%>
                                </td>
                            </tr>
                            <tr>
                                <td>Mobile</td>
                                <td>
                                    <%=data[i].person2_mobile%>
                                </td>
                            </tr>
                            <tr>
                                <td>Phone</td>
                                <td>
                                    <%=data[i].person2_phone%>
                                </td>
                            </tr>
                            <tr>
                                <td>Email</td>
                                <td>
                                    <%=data[i].person2_email%>
                                </td>
                            </tr>
                            <tr>
                                <td>Fax</td>
                                <td>
                                    <%=data[i].person2_fax%>
                                </td>
                            </tr>
                            <% } else { %>
                                <tr>
                                    <td>NIL</td>
                                </tr>
                                <% } %>
                    </table>
                </div>
                <div class="col-lg-3">
                    <table cellspacing="7" cellpadding="7" align="center">
                        <p><b>Contact Person 3</b></p>
                        <% if(data[i].person3_name){ %>
                            <tr>
                                <td>Name</td>
                                <td>
                                    <%=data[i].person3_name%>
                                </td>
                            </tr>
                            <tr>
                                <td>Designation</td>
                                <td>
                                    <%=data[i].person3_desg%>
                                </td>
                            </tr>
                            <tr>
                                <td>Mobile</td>
                                <td>
                                    <%=data[i].person3_mobile%>
                                </td>
                            </tr>
                            <tr>
                                <td>Phone</td>
                                <td>
                                    <%=data[i].person3_phone%>
                                </td>
                            </tr>
                            <tr>
                                <td>Email</td>
                                <td>
                                    <%=data[i].person3_email%>
                                </td>
                            </tr>
                            <tr>
                                <td>Fax</td>
                                <td>
                                    <%=data[i].person3_fax%>
                                </td>
                            </tr>
                            <% } else { %>
                                <tr>
                                    <td>NIL</td>
                                </tr>
                                <% } %>
                    </table>
                </div>
                <div class="col-lg-2"></div>
            </div>
            <% } %>
                <br>
                <hr style="width:80%;"><br>
                <!-- SCRIPTS -->
                <script></script>
                <!-- JQuery -->
                <script type="text/javascript" src="../../public/js/jquery-3.1.1.min.js"></script>
                <!-- Bootstrap tooltips -->
                <script type="text/javascript" src="../../public/js/tether.min.js"></script>
                <!-- Bootstrap core JavaScript -->
                <script type="text/javascript" src="../../public/js/bootstrap.min.js"></script>
                <!-- MDB core JavaScript -->
                <script type="text/javascript" src="../../public/js/mdb.min.js"></script>
</body>

</html>